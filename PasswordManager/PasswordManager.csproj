<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows10.0.22621.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>img\icons8-password-window-50.ico</ApplicationIcon>
    <Version>0.1.0</Version>
    <Authors>Topaze</Authors>
    <Description>Un gestionnaire de mot de passe simple</Description>
    <Copyright>2023</Copyright>
    <PackageIcon>icons8-password-window-50.png</PackageIcon>
    <NeutralLanguage>fr</NeutralLanguage>
    <StartupObject>PasswordManager.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="img\icons8-password-window-50.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="img\icons8-password-window-50.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>